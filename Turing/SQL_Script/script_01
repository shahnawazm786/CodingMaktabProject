create table tbl_employee
(
empno bigint primary key,
fname nvarchar(50),
lname nvarchar(50),
phone nvarchar(11),
email nvarchar(25),
isdeleted bit,
islocked bit
);

create table tbl_employee_det
(
empno bigint,
address nvarchar(max));

create table tbl_employee_gender
(
    empno bigint,
    sex char(1)
)
